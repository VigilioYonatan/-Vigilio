{
    "name": "@vigilio/express",
    "version": "1.2.7",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "description": "express decorators api",
    "bin": {
        "vigilio": "./scripts/main.js"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./package.json": "./package.json",
        "./helpers": {
            "import": "./dist/esm/helpers/index.js",
            "require": "./dist/cjs/helpers/index.js"
        },
        "./handler": {
            "import": "./dist/esm/handler/index.js",
            "require": "./dist/cjs/handler/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "helpers": [
                "./dist/esm/helpers/index.d.ts"
            ],
            "handler": [
                "./dist/esm/handler/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build": "rimraf dist && pnpm build:esm && pnpm build:cjs"
    },
    "peerDependencies": {
        "express": ">=4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VigilioYonatan/Vigilio/tree/main/preact-table#readme"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "scripts",
        "app.d.ts"
    ],
    "keywords": [
        "nest js",
        "express",
        "sequelize",
        "framework"
    ],
    "license": "ISC",
    "dependencies": {
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@decorators/di": "^2.0.0",
        "@decorators/express": "^2.9.4",
        "@types/express": "^4.17.17",
        "@types/formidable": "^3.4.0",
        "@types/node": "^20.4.2",
        "formidable": "^3.5.0"
    }
}
