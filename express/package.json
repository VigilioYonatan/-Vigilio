{
    "name": "@vigilio/express",
    "version": "1.0.8",
    "description": "express decorators api",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./helpers/*": {
            "types": "./dist/esm/helpers/*.d.ts",
            "import": "./dist/esm/helpers/*.js",
            "require": "./dist/cjs/helpers/*.js"
        }
    },
    "scripts": {
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build": "pnpm build:esm && pnpm build:cjs"
    },
    "peerDependencies": {
        "express": ">=4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VigilioYonatan/Vigilio/tree/main/preact-table#readme"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "preact",
        "typescript",
        "preact-table"
    ],
    "license": "ISC"
}
