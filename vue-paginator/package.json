{
    "name": "@vigilio/vue-paginator",
    "version": "0.0.1",
    "description": "this is a paginator library  to vue",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "peerDependencies": {
        "vue": ">=3.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VigilioYonatan/Vigilio/tree/main/vue-paginator#readme"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
